Option Explicit

Sub Get_Info()
Dim Msg As String
Dim url As String

url = "https://www.fiverr.com/share/Pl46bo"

Msg = "Capacity management tool" & Chr(10) & Chr(10) & "version: 2.4" & Chr(10) & Chr(10) & "Click yes to open developer webpage."

If MsgBox(Msg, vbInformation + vbYesNo, "Info!") <> vbYes Then Exit Sub

On Error Resume Next
    ThisWorkbook.FollowHyperlink (url)
On Error GoTo 0

End Sub

